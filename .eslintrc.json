{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "use": false,
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": ["prettier"],
  "rules": {
    "class-methods-use-this": 0,
    "prettier/prettier": "error",
    "strict": 0,
    "camelcase": ["error", { "properties": "never", "ignoreDestructuring": true }]
  }
}
